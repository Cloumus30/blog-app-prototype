    <!--blog end -->

    

    <!--Pagination_section start -->
    <div class="contact_main mb-4">
        <nav aria-label="Page navigation example" class="">
            <ul class="pagination justify-content-center">
                <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                
                <% let k = Math.ceil(posts.count/URLQuery.pageSize); let current =parseInt(URLQuery.page); let pgnStart =current-1; let pgnEnd=current+1;%>
                <!-- Limit the pagination Start -->
                <% if(current==1){ %> 
                    <% pgnStart = 1; pgnEnd = 3; %> 
                <% }%>
                <% if(current>=k-1){ %> 
                    <% pgnEnd = k; %> 
                <% }%> 
                <!-- Limit The Pagination End -->

                <% for(let j=pgnStart; j<=pgnEnd; j++){ %>
                    <% if(URLQuery.page == j){ %>
                        <li class="page-item active"> <a class="page-link" href="/?page=<%=j%>&pageSize=<%= URLQuery.pageSize %>"><%=j%></a> </li>
                    <% }else{ %>  
                        <li class="page-item"> <a class="page-link" href="/?page=<%=j%>&pageSize=<%= URLQuery.pageSize %>"><%=j%></a> </li>
                    <% } %> 
                <% }%>

                <li class="page-item"><a class="page-link" href="#">Next</a></li>
            </ul>
          </nav>    
    </div>
