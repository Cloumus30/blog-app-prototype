            <div class="row">

                <% posts.rows.forEach(post => { %>
                    <div class="col-sm-4">
                        <div class="section_1">
                            <a href="/post/<%= post.id %>"><img height="250px" src="<%=post.image%>" style="max-width: 100%;"></a>
                            <!-- Updated At -->
